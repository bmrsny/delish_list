<div class="about-box">
	 <% if current_user.present? %>
		<h2><%= current_user.name %>'s Shopping Lists</h2>
		<h3>Select a shopping list</h3>
	<div>
		<% @shopping_lists.each do |shopping_list| %>
			<ul>
				<li><%= link_to shopping_list.name, shopping_list_path(shopping_list) %></li>	
			</ul>
		<% end %>
	</div>
	<% end %>
</div>
